{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "eslint:recommended",
        "@vue/typescript/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": [
    ],
    "rules": {
        "no-console": "error",
        "vue/html-indent": [
            "warn",
            4
        ],
        "vue/script-indent": [
            "warn",
            4,
            {
                "switchCase": 1
            }
        ]
    }
}
